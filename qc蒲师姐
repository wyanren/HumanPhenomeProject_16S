 --i-table table_rep_dada2-QC-collapse-rm_singletons-filter_20000.qza \
--i-taxonomy taxonomy.qza \
--m-metadata-file metadata-sum-collapse.txt \
--o-visualization vsearch_bar-plots_rm_singletons-filter_20000.qzv

